<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff414141" android:endColor="#ff414141" android:angle="270.0" android:centerY="1.0" android:centerColor="#ff414141" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff006175" android:endColor="#ff006175" android:angle="270.0" android:centerY="1.0" android:centerColor="#ff006175" />
            </shape>
        </clip>
    </item>
</layer-list>