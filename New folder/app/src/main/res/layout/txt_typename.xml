<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="160.0dip" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:gravity="center_vertical"
  android:focusable="true">
    <ImageView 
        android:id="@+id/img_listseparate"
	    android:layout_width="fill_parent"
	    android:layout_height="1.0dip"
	    android:layout_marginTop="3.0dip"
	    android:layout_marginLeft="15.0dip"
	    android:layout_marginRight="15.0dip"
	    android:background="#90FFFFFF"/>
    <LinearLayout 
      android:id="@+id/txt_back_set_layout"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:orientation="horizontal"
      android:layout_marginTop="3.0dip"
      android:layout_marginLeft="15.0dip"
      android:layout_marginRight="15.0dip"
      android:gravity="center_vertical">
	  <ImageView 
		    android:id="@+id/img_type_name"
		    android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:src="@drawable/iconpic"
	        android:layout_marginLeft="10.0dip"/>
	    <TextView
	        android:id="@id/txt_type_name"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10.0dip"
	        android:background="@drawable/selector_typename"
	        android:gravity="left"
	        android:singleLine="true"
	        android:text="UK"
	        android:textColor="@color/white"
	        android:textSize="22.0sp"
	     />
	</LinearLayout>
</LinearLayout>